use dataverse graph;

insert into dataset MsgGraph
(
   for $tm in dataset TwitterMessages
   let $edge := switch-case($tm.forward-from>=0, true,
               $tm.forward-from, false, $tm.retweet-from)
   where $edge>=0
   return {
        "vertexid": $tm.tweetid,
        "value": 1.0/10000000000.0,
        "edges": [$edge]
   }
);