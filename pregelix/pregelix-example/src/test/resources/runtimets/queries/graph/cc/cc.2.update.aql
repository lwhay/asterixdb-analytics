use dataverse graph;

load dataset rawweb 
using "org.apache.asterix.external.dataset.adapter.NCFileSystemAdapter"
(("path"="nc1://data/webmap_json/webmap_link.adm"),("format"="adm")) pre-sorted;

insert into dataset webmap(
	for $w in dataset("rawweb")
	order by $w.id
	return {
   		"vertexid": $w.id,
   		"value": $w.id,
   		"edgelist": $w.edges
	}
);